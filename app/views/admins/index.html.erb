<%=
if params.has_key?(:id)
    rolesUsername = get_user_name(params[:id])
    rolesUserrole = get_user_role(params[:id])
    rolesAvailable = get_role_list
    existingRestaurant = get_existing_restaurant(params[:id])
else
    rolesUsername = nil
    rolesUserrole = nil
    rolesAvailable = nil
    existingRestaurant = nil
end

if params.has_key?(:rid)
    riderUsername = get_user_name(params[:rid])
    partTimeSchedule = nil
    fullTimeSchedule = nil
    riderSchedule = get_rider_schedule_matrix(params[:rid])
else
    riderUsername = nil
    partTimeSchedule = get_part_time_schedule_matrix
    fullTimeSchedule = get_full_time_schedule_matrix
    riderSchedule = nil
end

react_component(
    'admins/Index',
    currentUser: current_user,
    roleAttributes: get_current_user_role_attributes('admin'),
    statistics: get_statistics,
    params: params,
    rolesUsername: rolesUsername,
    rolesUserrole: rolesUserrole,
    rolesAvailable: rolesAvailable,
    existingRestaurant: existingRestaurant,
    allPromos: get_all_fds_promos,
    riderUsername: riderUsername,
    partTimeSchedule: partTimeSchedule,
    fullTimeSchedule: fullTimeSchedule,
    riderSchedule: riderSchedule
)
%>
