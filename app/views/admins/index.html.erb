<%=
if params.has_key?(:id)
    rolesUsername = get_user_name(params[:id])
    rolesUserrole = get_user_role(params[:id])
else
    rolesUsername = nil
    rolesUserrole = nil
end

react_component(
    'admins/Index',
    currentUser: current_user,
    roleAttributes: get_current_user_role_attributes('admin'),
    statistics: get_statistics,
    params: params,
    rolesUsername: rolesUsername,
    rolesUserrole: rolesUserrole
)
%>
